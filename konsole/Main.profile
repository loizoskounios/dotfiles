[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=PaulMillr
Font=Fira Code,14,-1,5,50,0,0,0,0,0
LineSpacing=5
UseFontLineChararacters=false

[Cursor Options]
CursorShape=1
CustomCursorColor=255,255,255
UseCustomCursorColor=true

[General]
Command=/bin/zsh
Directory=$HOME
Environment=TERM=xterm-256color,COLORTERM=truecolor,SHELL=/bin/zsh
Name=Main
Parent=FALLBACK/
ShowTerminalSizeHint=false
StartInCurrentSessionDir=false
TerminalMargin=50

[Interaction Options]
AutoCopySelectedText=true
MiddleClickPasteMode=1
MouseWheelZoomEnabled=false
OpenLinksByDirectClickEnabled=true
UnderlineFilesEnabled=true

[Keyboard]
KeyBindings=linux

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
UrlHintsModifiers=0
